{
    "name": "koenhoeijmakers/laravel-translatable",
    "description": "Laravel Translations",
    "license": "MIT",
    "authors": [
        {
            "name": "Koen Hoeijmakers",
            "email": "koen@hoeijmakers.me"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.5",
        "laravel/framework": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^5.0",
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "KoenHoeijmakers\\LaravelTranslatable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KoenHoeijmakers\\LaravelTranslatable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KoenHoeijmakers\\LaravelTranslatable\\TranslatableServiceProvider"
            ]
        }
    }
}
